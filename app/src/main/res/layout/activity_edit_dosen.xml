<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditDosenActivity">

    <TextView
        android:id="@+id/txtNamaDosen"
        android:layout_width="91dp"
        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="76dp"
        android:text="Nama"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtNidnDosen"
        android:layout_width="91dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="NIDN"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNamaDosen" />

    <TextView
        android:id="@+id/txtAlamatDosen"
        android:layout_width="91dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="Alamat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNidnDosen" />

    <TextView
        android:id="@+id/txtEmailDosen"
        android:layout_width="91dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="Email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAlamatDosen" />

    <TextView
        android:id="@+id/txtGelarDosen"
        android:layout_width="91dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="Gelar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEmailDosen" />

    <TextView
        android:id="@+id/txtFotoDosen"
        android:layout_width="91dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        android:text="Foto"
        app:layout_constraintBottom_toTopOf="@+id/btnSimpanDosen"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtGelarDosen"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/etxtNamaDosen"
        android:layout_width="200dp"
        android:layout_height="24dp"
        android:layout_marginTop="76dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.84"
        app:layout_constraintStart_toEndOf="@+id/txtNamaDosen"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etxtNidnDosen"
        android:layout_width="200dp"
        android:layout_height="24dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.84"
        app:layout_constraintStart_toEndOf="@+id/txtNidnDosen"
        app:layout_constraintTop_toBottomOf="@+id/etxtNamaDosen" />

    <EditText
        android:id="@+id/etxtAlamatDosen"
        android:layout_width="200dp"
        android:layout_height="24dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.84"
        app:layout_constraintStart_toEndOf="@+id/txtAlamatDosen"
        app:layout_constraintTop_toBottomOf="@+id/etxtNidnDosen" />

    <EditText
        android:id="@+id/etxtEmailDosen"
        android:layout_width="200dp"
        android:layout_height="24dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.84"
        app:layout_constraintStart_toEndOf="@+id/txtEmailDosen"
        app:layout_constraintTop_toBottomOf="@+id/etxtAlamatDosen" />

    <EditText
        android:id="@+id/etxtGelarDosen"
        android:layout_width="200dp"
        android:layout_height="24dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.84"
        app:layout_constraintStart_toEndOf="@+id/txtGelarDosen"
        app:layout_constraintTop_toBottomOf="@+id/etxtEmailDosen" />



    <Button
        android:id="@+id/btnBrowse"
        android:layout_width="93dp"
        android:layout_height="35dp"
        android:layout_marginTop="24dp"
        android:text="Browse"
        app:layout_constraintBottom_toTopOf="@+id/btnSimpanDosen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/etxtFotoDosen"
        app:layout_constraintTop_toBottomOf="@+id/etxtGelarDosen"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnSimpanDosen"
        android:layout_width="369dp"
        android:layout_height="49dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="200dp"
        android:text="Simpan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>